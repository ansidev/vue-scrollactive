{
  "name": "vue-scrollactive",
  "version": "0.8.0",
  "title": "Vue Scrollactive",
  "description": "Lightweight and simple to use vue component that highlights menu items as you scroll the page, also scrolling to target section when clicked.",
  "main": "dist/vue-scrollactive.min.js",
  "scripts": {
    "dev": "webpack --progress --hide-modules --mode=development",
    "watch": "webpack --progress --hide-modules --watch --mode=development",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "prod": "webpack --progress --hide-modules --mode=production"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eddiemf/vue-scrollactive.git"
  },
  "keywords": [
    "vue",
    "vue-plugin",
    "vue-component",
    "scrollspy",
    "scroll",
    "active",
    "active-class",
    "menu-item",
    "highlight-menu-item"
  ],
  "author": {
    "name": "Mauricio Farias Dziedzinski",
    "email": "d.mauriciofarias@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eddiemf/vue-scrollactive/issues"
  },
  "homepage": "https://github.com/eddiemf/vue-scrollactive#readme",
  "dependencies": {
    "bezier-easing": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "eslint": "^5.12.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-html": "^5.0.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-vue": "^5.1.0",
    "vue-loader": "^15.6.0",
    "vue-template-compiler": "^2.5.22",
    "webpack": "4.29.0",
    "webpack-cli": "^3.2.1"
  }
}
